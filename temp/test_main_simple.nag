def fibonacci_recursive(n):
    if n <= 0:
        return 0
    if n == 1:
        return 1
    return fibonacci_recursive(n - 1) + fibonacci_recursive(n - 2)

def main():
    print("Test")
    # Test numbers 0 through 10
    i = 0
    while i <= 10:
        result = fibonacci_recursive(i)
        print("fibonacci(" + str(i) + ") =", result)
        i = i + 1

main()

# Simple test for data structures and functions

def test_lists():
    numbers = [1, 2, 3, 4, 5]
    print("Original list:", numbers)

    # Test list operations (using JavaScript-style push for now)
    doubled = []
    for num in numbers:
        doubled.push(num * 2)

    print("Doubled list:", doubled)
    return doubled

def test_dictionaries():
    person = {
        "name": "Alice",
        "age": 30,
        "city": "New York"
    }

    print("Person data:", person)
    print("Name:", person["name"])
    print("Age:", person["age"])

    return person

def main():
    print("=== Testing Lists ===")
    test_lists()

    print("=== Testing Dictionaries ===")
    test_dictionaries()

    print("=== All tests completed ===")

main()

def fibonacci(n: int) -> int:
    if n <= 1:
        return n
    else:
        return fibonacci(n - 1) + fibonacci(n - 2)

def test_fibonacci():
    test_cases = [
        (0, 0),
        (1, 1),
        (2, 1),
        (3, 2),
        (4, 3),
        (5, 5),
        (6, 8),
        (7, 13),
        (8, 21),
        (9, 34),
        (10, 55)
    ]

    print("🧪 Testing Fibonacci Function")
    print("=" * 40)

    passed = 0
    failed = 0

    for n, expected in test_cases:
        result = fibonacci(n)
        if result == expected:
            print("✓ fibonacci(" + str(n) + ") = " + str(result))
            passed = passed + 1
        else:
            print("✗ fibonacci(" + str(n) + ") = " + str(result) + ", expected " + str(expected))
            failed = failed + 1

    print("")
    print("Results: " + str(passed) + "/" + str(passed + failed) + " tests passed")

    if failed == 0:
        print("🎉 All tests passed!")
    else:
        print("⚠️ " + str(failed) + " tests failed")

test_fibonacci()
